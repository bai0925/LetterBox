<!--这里将数据库名指定成cool_weather，数据库版本指定成1，
并将Province、City和County这3个实体类添加到映射列表中。-->

<litepal>
    <dbname value="cool_weather"/>
    <version value="1"/>
    <list>
        <mapping class="com.db.Province"/>
        <mapping class="com.db.City"/>
        <mapping class="com.db.County"/>
    </list>
</litepal>